
@model AppMVCGrupo12.Models.Empleado
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <form class="form-control" method="post" action="/Home/Capturar">
            <div>
                <input name="idEmpleado" placeholder="Id Empleado" type="text" value="@Model.idEmpleado" />
                <input name="nombreEmpleado" placeholder="Nombre del Empleado" type="text" value="@Model.nombreEmpleado" />
                <input name="sueldoEmpleado" placeholder="Sueldo del Empleado" type="text" value="@Model.sueldoEmpleado" />
            </div>

            <div>
                <input name="submit" type="submit" value="Aceptar" />
            </div>

            <div>
                @ViewBag.msg
            </div>
        </form>
    </div>

    <table>
        <tr>
            <td>
                Código
            </td>
            <td>
                Nombre
            </td>
        </tr>
        @foreach (var item in ViewBag.modelo2)
        {
            <tr>
                <td>@item.idEmpleado</td>
                <td>@item.nombreEmpleado</td>
            </tr>
        }

    </table>
</body>
</html>
